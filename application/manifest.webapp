  "description": "chelaile for KaiOS",
  "version": "1.0",
  "launch_path": "/index.html",
  "type": "privileged",
  "origin": "app://kai.michael.chelaile",
  "icons": {
    "120": "/img/icon120x120.png"
  },
  "developer": {
    "name": "michael_eddy",
    "url": "https://www.chelaile.net.cn/"
  },
  "permissions": {
    "attention": {},
    "browser": { "description": "Required for browsing webpages" },
