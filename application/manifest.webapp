    },
    "spatialnavigation-app-manage": {}
  },
  "fullscreen": false,
  "ime_default_softkey_bar": true,
  "theme_color": "#00a1d6",
  "default_locale": "zh-CN",
  "locales": {
    "zh-CN": {
      "name": "车来了",
